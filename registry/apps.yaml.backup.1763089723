    pattern: single-vm
    status: production
    maintainer: tfgrid-studio
    tags:
    - ai
    - coding
    - agent
    - qwen
    version: cffec44
    versioning:
      primary: git_commit
      fallback: semantic
      auto_update: true
    git:
      default_branch: main
      tracking: true
      auto_cache_refresh: true
  - name: tfgrid-gitea
    repo: github.com/tfgrid-studio/tfgrid-gitea
    description: Self-hosted Git service with web interface
    pattern: single-vm
    status: production
    maintainer: tfgrid-studio
    tags:
    - git
    - code
    - hosting
    - self-hosted
    version: efcfdd2
    versioning:
      primary: git_commit
      fallback: semantic
      auto_update: true
    git:
      default_branch: main
      tracking: true
